    <item>
      <title>Fix multiple consecutive line breaks</title>
      <guid>urls</guid>
      <content:encoded><![CDATA[
        <p>Foo<br />
        <br />
        <br />
        <br />
        <br />
        <br />
        Bar</p>
        Foo<br /><br /><br /><br /><br /><br />Bar
        <p>Foo<br /><br />Bar</p>
        Foo<br /><br />Bar
      ]]></content:encoded>
    </item>
